{
    "name": "devedge/photo-desc",
    "description": "A PHP script that reads photos from an input folder and uses OpenRouter to classify them",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "psr/http-client": "^1.0",
        "psr/http-message": "^1.0",
        "psr/log": "^1.1 || ^2.0 || ^3.0"
    },
    "require-dev": {
        "vlucas/phpdotenv": "^5.5",
        "king23/di": "^2.0",
        "guzzlehttp/guzzle": "^7.5",
        "monolog/monolog": "^2.9",
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "PhotoDesc\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PhotoDesc\\Tests\\": "tests/"
        }
    }
}
